{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2017",
    "module": "ES2022",
    "lib": [
      "es2022",
      "DOM",
      "WebWorker",
      "DOM.Iterable"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "types": [
      "node",
      "vite/client"
    ],
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "incremental": true,
    "isolatedModules": true,
    "outDir": "tmp",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "./*.d.ts",
    "./*.config.ts"
  ],
  "exclude": [
    "tmp"
  ]
}
